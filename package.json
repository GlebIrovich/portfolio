    "hover.css": "^2.3.2",
    "lodash.shuffle": "^4.2.0",
    "node-sass-chokidar": "^1.3.0",
    "npm-run-all": "^4.1.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dimensions": "^1.3.1",
    "react-dom": "^16.4.1",
    "react-fontawesome": "^1.6.1",
    "react-pose": "^2.1.2",
    "react-router": "^4.3.1",
